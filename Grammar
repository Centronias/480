	IFSTMTS
	WHILESTMTS
	LETSTMTS
	PRINTSTMTS
PRINTSTMTS
	(stdout OPER)
IFSTMTS
	(if EXPR EXPR EXPR)
	(if EXPR EXPR)
WHILESTMTS
	(while EXPR EXPRLIST)
EXPRLIST
	EXPR
	EXPR EXPRLIST
LETSTMTS
	(let (VARLIST))
VARLIST
	(NAME type)
	(NAME type) VARLIST